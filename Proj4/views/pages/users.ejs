<!DOCTYPE html>
<html>
<head>
    <title>All Users</title>

</head>
<body>
	<%- include('../partials/header'); -%>

	<br>

  <% for (let user in users) { %>
    <% if(users[user].privacy==false){ %>
  			<a href="/users/<%=users[user]._id%>"><%=users[user].username%></a>
  			<br>
    <% } %>
  <% }; %>

	<%- include('../partials/footer'); -%>

  <script src="client.js"></script>
</body>
</html>
